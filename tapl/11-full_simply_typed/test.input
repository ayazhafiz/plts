numeric = letrec ieio: { iseven: Nat->Bool, isodd: Nat->Bool } =
  { iseven = lambda x: Nat.
      if (iszero x) then true
      else ieio.isodd (pred x),

    isodd = lambda x: Nat.
      if (iszero x) then false
      else ieio.iseven (pred x) }
  in ieio;

